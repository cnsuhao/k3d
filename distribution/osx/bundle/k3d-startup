echo "running $0 with arguments $@"

CWD="`(cd \"\`dirname \\\"$0\\\"\`\"; echo $PWD)`"
TOP="`dirname \"$CWD\"`"

export DYLD_LIBRARY_PATH="$TOP/lib"
#export PATH="$CWD:$PATH"

exec "$CWD/k3d" "$@"

